{
  "name": "OptimEyes",
  "description": "A reminder to rest your eyes from your computer every once in a while.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab", 
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "OptimEyes Logo 2.png",
    "default_title": "Start Session"
  }
}